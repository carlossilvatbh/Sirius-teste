{
  "name": "sirius-frontend",
  "version": "2.0.0",
  "type": "module",
  "description": "Advanced Legal Structure Configuration Platform - Frontend",
  "author": "Sirius Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "vite-bundle-analyzer dist/stats.html"
  },
  "keywords": [
    "legal-structures",
    "tax-optimization",
    "compliance",
    "business-formation",
    "vue",
    "typescript",
    "vite"
  ],
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.45.0",
    "@vue-flow/minimap": "^1.5.3",
    "@vueuse/core": "^13.5.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vue-tsc": "^3.0.1"
  }
}
